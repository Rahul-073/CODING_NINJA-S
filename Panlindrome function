#include <iostream>
#include <cstring>
using namespace std;

bool checkPalindrome(char str[]) {
    int n;
    bool check=1;
    for(int i=0;i<n;i++){
        if (str[i]!=str[n-1-i]){
            return false;
            
        }
    }
    return true;
    
}

int main() {
    int size = 1e6;
    char str[size];
    cin >> str;
    cout << (checkPalindrome(str) ? "true" : "false");
}
