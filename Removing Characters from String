#include<bits/stdc++.h>
using namespace std;

void removeAllOccurrencesOfChar(string input, char c) {
  string NS;
  for (int i=0; i<input.size();i++){
    if(input.at(i)!=c){
      NS+=input.at(i);
    }
  }
  cout<< NS;
}


int main(){
    string a;
    cin>>a;
    char c;
    cin>>c;
    removeAllOccurrencesOfChar(a,c);
}
